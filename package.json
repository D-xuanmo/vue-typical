{
  "name": "@Turkyden/vue-typical",
  "version": "1.4.3",
  "description": "Vue Animated typing in ~400 bytes 🐡 of JavaScript.",
  "author": "Turkyden",
  "license": "MIT",
  "repository": "Turkyden/vue-typical",
  "homepage": "https://turkyden.github.io/vue-typical",
  "private": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://github.com/Turkyden"
  },
  "main": "dist/typical.umd.js",
  "module": "dist/typical.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name typical src/typical.js",
    "build:docs": "vue-cli-service build --dest build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@camwiegert/typical": "^0.1.1",
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
